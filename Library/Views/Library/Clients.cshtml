@model IEnumerable<Library.Models.Client>
@{
    ViewData["Title"] = "Клиенты";
}

<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<table>
    <thead>
        <tr>
            <th>Идентификатор</th>
            <th>ФИО</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Library.Models.Client client in Model)
        {
            <tr>
                <td>@client.ClientId</td>
                <td>@client.ClientName</td>
            </tr>
        }
    </tbody>
</table>